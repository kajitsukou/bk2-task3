<h1>New book</h1>
<!-- form_for部分 -->
        <%= form_for(book) do |f| %>
        <% if book.errors.any? %>
        <%= book.errors.count %>error
        <% book.errors.full_messages.each do |message| %>
        <%= message %>
        <% end %>
        <% end %>

　　　　<div class="row">
        <%= f.label :title %>
        <%= f.text_field :title, class:"col-xs-12" %>
      </div>

      <div class="row">
          <%= f.label :body %>
          <%= f.text_area :body,class:"col-xs-12" %>
      </div>

      <div class="row">
          <%= f.submit 'Create Book',class:"btn btn-primary col-xs-12" %>
      </div>
        <% end %>